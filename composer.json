{
    "name": "bnf/nginx-cache",
    "type": "typo3-cms-extension",
    "description": "NGINX Cache Manager for TYPO3",
    "keywords": [
        "TYPO3",
        "extension"
    ],
    "homepage": "https://bnf.dev",
    "authors": [
        {
            "name": "Benjamin Franzke",
            "email": "ben@bnf.dev",
            "role": "Developer",
            "homepage": "https://bnf.dev"
        }
    ],
    "license": "GPL-2.0+",
    "require": {
        "typo3/cms-core": "^12.4.0 || ~13.0.0",
        "typo3/cms-frontend": "^12.4.0 || ~13.0.0",
        "typo3/cms-backend": "^12.4.0 || ~13.0.0"
    },
    "autoload": {
        "psr-4": {
            "Bnf\\NginxCache\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/nginx-cache": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "nginx_cache"
        }
    }
}
